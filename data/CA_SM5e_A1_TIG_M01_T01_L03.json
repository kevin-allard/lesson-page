      "overview_item_title": "My Learning Goals",
      "overview_item_data": {
        "overview_item_data_type": "list",
        "overview_item_data_items": [
          "I can define a function.",
          "I can identify a function, in function notation, for a given graph.",
          "I can determine and recognize characteristics of functions and function families."
        ]
      }
