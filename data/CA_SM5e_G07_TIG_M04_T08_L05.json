    "overview_number": 5,
    "overview_title": "Unique or Not?",
    "overview_subtitle": "Constructing Triangles Given Angles",
    "overview_description": "In this lesson, students use patty paper and construction tools to investigate triangles. They determine the conditions needed to produce unique triangles, more than one triangle, or no triangles when given at least one angle."
  },
  "overview_items": [
    {
      "overview_item_type": "learning_goals",
      "overview_item_content_type": "html",
      "overview_item_title": "My Learning Goals",
      "overview_item_data": {
        "overview_item_data_type": "list",
        "overview_item_data_items": [
          "I can use patty paper and technology to investigate triangles.",
          "I can determine whether given segments and angles form a unique triangle, more than one triangle, or no triangle.",
          "I can construct triangles using given conditions."
        ]
      }
    },
