      }
    },
    {
      "overview_item_type": "essential_ideas",
      "overview_item_content_type": "html",
      "overview_item_title": "Essential Ideas",
      "overview_item_data": {
        "overview_item_data_type": "list",
        "overview_item_data_items": [
          "Constructing a triangle given the measure of three angles does not result in a unique triangle.",
          "Constructing a triangle given the measure of two angles and one side length does not result in a unique triangle.",
          "An included angle is the angle whose sides consist of the specified sides of a triangle.",
          "An included side is the side between two specified angles of a triangle.",
          "Constructing a triangle given the length of two sides and the measure of the included angle results in a unique triangle.",
          "Constructing a triangle given the measure of two angles and the included side length results in a unique triangle."
        ]
      }
    },
