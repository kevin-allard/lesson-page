      "overview_item_content_type": "html",
      "overview_item_title": "My Learning Goals",
      "overview_item_data": {
        "overview_item_data_type": "list",
        "overview_item_data_items": [
          "I can expand powers into products and write products as powers.",
          "I can explain the Properties of Powers, including Product of Powers, Quotient of Powers, and Power of a Power rules.",
          "I can apply the properties of integer exponents to create and rewrite equivalent expressions."
        ]
      }
    },
