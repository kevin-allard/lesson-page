  "overview_metadata": {
    "overview_number": 1,
    "overview_title": "Way to Represent!",
    "overview_subtitle": "Representing and Analyzing Data",
    "overview_description": "In this lesson, students review the statistical process by constructing, analyzing, and comparing dot plots and histograms. They use two five-number summaries to create and analyze box-and-whisker plots. Students revisit how data distribution affects the best measure of center and recalculate the IQR after removing outliers."
  },
  "overview_items": [
    {
      "overview_item_type": "learning_goals",
      "overview_item_content_type": "html",
      "overview_item_title": "My Learning Goals",
      "overview_item_data": {
        "overview_item_data_type": "list",
        "overview_item_data_items": [
          "I can represent and compare data represented on data displays.",
          "I can determine and interpret the mean and median of a data set.",
          "I can determine and interpret the interquartile range (IQR) of a data set.",
          "I can determine which measure of central tendency is best to use for a data set."
        ]
      }
    },
    {
      "overview_item_type": "big_ideas",
      "overview_item_content_type": "text",
      "overview_item_title": "Big Idea",
      "overview_item_data": {
        "overview_item_data_type": "list",
