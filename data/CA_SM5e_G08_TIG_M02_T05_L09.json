  "overview_metadata": {
    "overview_number": 9,
    "overview_title": "What's the Point?",
    "overview_subtitle": "Writing and Graphing Linear Equations",
    "overview_description": "In this lesson, students learn how to create the slope-intercept and point-slope forms of a line from the slope formula. They write equations in each form from tables, graphs, situations, two points, or a slope and a point. Students graph lines using a slope and the <math><mi>y</mi></math>-intercept or a slope and a point. They make sense of the graphs of horizontal and vertical lines by relating them to their slopes, tables, and equations."
  },
  "overview_items": [
    {
      "overview_item_type": "learning_goals",
      "overview_item_content_type": "html",
      "overview_item_title": "My Learning Goals",
      "overview_item_data": {
        "overview_item_data_type": "list",
        "overview_item_data_items": [
          "I can write equations of lines in slope-intercept and point-slope form.",
          "I can write equations for vertical and horizontal lines.",
          "I can graph lines using the slope-intercept or point-slope form of a linear equation."
        ]
      }
    },
    {
      "overview_item_type": "big_ideas",
      "overview_item_content_type": "text",
