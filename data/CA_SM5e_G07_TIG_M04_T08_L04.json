      "overview_item_content_type": "html",
      "overview_item_title": "Essential Ideas",
      "overview_item_data": {
        "overview_item_data_type": "list",
        "overview_item_data_items": [
          "Constructing a triangle given the length of two sides does not result in a unique triangle.",
          "Constructing a triangle given the length of three segments such that the sum of two segment lengths is greater than the third length results in a unique triangle.",
          "The sum of the lengths of any two sides of a triangle is greater than the third side's length."
        ]
      }
