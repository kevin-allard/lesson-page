  "overview_metadata": {
    "overview_number": 4,
    "overview_title": "The Forest for the Trees",
    "overview_subtitle": "Determining the Part and the Whole in Percent Problems",
    "overview_description": "In this lesson, students solve percent problems for the part or the whole. They conclude that the most efficient methods to determine the part are multiplying the whole by the decimal equivalent of the percent or using benchmark percents. Students use double number lines and proportions to calculate the whole in area, volume, and surface area problems."
  },
  "overview_items": [
    {
      "overview_item_type": "learning_goals",
      "overview_item_content_type": "html",
      "overview_item_title": "My Learning Goals",
      "overview_item_data": {
        "overview_item_data_type": "list",
        "overview_item_data_items": [
          "I can solve percent problems for the part given the whole and the percent and for the whole given the part and percent.",
          "I can use ratio and rate reasoning to solve percent problems."
        ]
      }
    },
