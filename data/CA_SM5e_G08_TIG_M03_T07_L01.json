      "overview_item_data": {
        "overview_item_data_type": "list",
        "overview_item_data_items": [
          "I can write sequences of numbers generated from the creation of diagrams and written contexts.",
          "I can state varying growth patterns of sequences.",
          "I can use graphs to represent sequences."
        ]
      }
    },
