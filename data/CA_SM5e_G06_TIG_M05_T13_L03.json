    "overview_number": 3,
    "overview_title": "Box It Up",
    "overview_subtitle": "Displaying the Five-Number Summary",
    "overview_description": "In this lesson, students construct box-and-whisker plots and analyze and interpret the data displays by considering two measures of variation: the range and the IQR. They also analyze data sets using the five-number summary."
  },
  "overview_items": [
    {
      "overview_item_type": "learning_goals",
      "overview_item_content_type": "html",
      "overview_item_title": "My Learning Goals",
      "overview_item_data": {
        "overview_item_data_type": "list",
        "overview_item_data_items": [
          "I can describe an overall pattern of data.",
          "I can calculate and interpret measures of variation for a data set, including range, quartiles, interquartile range, and the five-number summary.",
          "I can create box-and-whiskers plots to display a set of numeric data."
        ]
      }
    },
    {
