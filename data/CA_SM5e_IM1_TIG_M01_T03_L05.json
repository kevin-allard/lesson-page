      "overview_item_content_type": "html",
      "overview_item_title": "My Learning Goals",
      "overview_item_data": {
        "overview_item_data_type": "list",
        "overview_item_data_items": [
          "I can create and analyze scatter plots with appropriate accuracy, describing how the variables are related.",
          "I can fit a function to data and use it to solve problems.",
          "I can determine whether a function is a good fit for a data set.",
          "I can use residual plots to help determine whether a linear regression is the best fit for data."
        ]
      }
    },
