    {
      "overview_item_type": "essential_ideas",
      "overview_item_content_type": "html",
      "overview_item_title": "Essential Ideas",
      "overview_item_data": {
        "overview_item_data_type": "list",
        "overview_item_data_items": [
          "The Triangle Sum Theorem states that the sum of the measures of the interior angles of a triangle is 180°.",
          "The longest side of a triangle lies opposite the largest interior angle.",
          "The shortest side of a triangle lies opposite the smallest interior angle.",
          "The remote interior angles of a triangle are the two angles non-adjacent to the exterior angle.",
          "The Exterior Angle Theorem states that the measure of the exterior angle of a triangle is equal to the sum of the measures of the two remote interior angles of the triangle."
        ]
      }
    },
