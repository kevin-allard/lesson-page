  "overview_metadata": {
    "overview_number": 1,
    "overview_title": "Construction Ahead",
    "overview_subtitle": "Constructing a Square",
    "overview_description": "In this lesson, students distinguish constructions from sketches and drawings. They construct segments, rays, lines, and circles, as well as perpendicular lines using patty paper and a compass and straightedge. Students then construct a square inscribed in a circle using both rotations of a right triangle on the plane and a compass and straightedge construction."
  },
  "overview_items": [
    {
      "overview_item_type": "learning_goals",
      "overview_item_content_type": "html",
      "overview_item_title": "My Learning Goals",
      "overview_item_data": {
        "overview_item_data_type": "list",
        "overview_item_data_items": [
          "I can construct the perpendicular bisector of a segment and describe its properties.",
          "I can use construction tools to duplicate a line segment and construct a segment bisector.",
          "I can construct a perpendicular line through a point on a line and through a point not on a line.",
          "I can construct a square inscribed in a circle using construction tools and geometric reasoning."
        ]
      }
    },
    {
