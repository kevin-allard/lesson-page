  "overview_metadata": {
    "overview_number": 7,
    "overview_title": "Complying with Title IX",
    "overview_subtitle": "Constant of Proportionality",
    "overview_description": "In this lesson, students explore equations of proportional relationships. They determine the constant of proportionality, the constant ratio of the outputs to the inputs in a proportional relationship. Students explore the reciprocal relationships of constants of proportionality in equations. They then use the constant of proportionality to write and solve equations."
  },
  "overview_items": [
    {
      "overview_item_type": "learning_goals",
      "overview_item_content_type": "html",
      "overview_item_title": "My Learning Goals",
      "overview_item_data": {
        "overview_item_data_type": "list",
        "overview_item_data_items": [
          "I can identify the constant of proportionality in proportional relationships.",
          "I can determine whether there is a constant ratio between two variables.",
          "I can represent proportional relationships with equations."
        ]
      }
    },
    {
      "overview_item_type": "big_ideas",
      "overview_item_content_type": "text",
      "overview_item_title": "Big Idea",
      "overview_item_data": {
        "overview_item_data_type": "list",
